msgstr "클릭가능"

#: controlTypes.py:506
msgid "editable"
msgstr "편집가능"

#: controlTypes.py:507
msgid "checkable"
msgstr "체크가능"

#: controlTypes.py:508
msgid "draggable"
msgstr "드래깅 가능"

#: controlTypes.py:509
msgid "dragging"
msgstr "드래그 중"

#. Translators: Reported where an object which is being dragged can be dropped.
#. This is only reported for objects which support accessible drag and drop.
#: controlTypes.py:512
msgid "drop target"
msgstr "drop target"

#: controlTypes.py:513
msgid "sorted"
msgstr "정렬"

#: controlTypes.py:514
msgid "sorted ascending"
msgstr "오름차순 정렬"

#: controlTypes.py:515
msgid "sorted descending"
msgstr "내림차순 정렬"

#. Translators: a state that denotes that an object (usually a graphic) has a long description.
#: controlTypes.py:517
msgid "has long description"
