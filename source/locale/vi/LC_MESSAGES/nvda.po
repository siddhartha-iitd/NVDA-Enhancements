msgid "Activates the NVDA Python Console, primarily useful for development"
msgstr "Kích hoạt NVDA Python Console, ưu tiên cho việc phát triển NVDA."

msgid "review"
msgstr "xem lại"

msgid "focus"
msgstr "focus"

msgid "Braille tethered to %s"
msgstr "Hạn chế Braille vào %s"

msgid "Toggle tethering of braille between the focus and the review position"
msgstr "Tắt bật giới hạn Braille giữa focus và vị trí xem lại"

msgid "There is no text on the clipboard"
msgstr "Không có tài liệu trong bộ nhớ tạm"

msgid "The clipboard contains a large portion of text. It is %s characters long"
msgstr "Bộ nhớ tạm có phần lớn văn bản. Nó có chiều dài số ký tự là %s"

msgid "Reports the text on the Windows clipboard"
msgstr "Thông báo văn bản trên bộ nhớ tạm cửa sổ."

msgid "Start marked"
msgstr "Bắt đầu được đánh dấu"

msgid "Marks the current position of the review cursor as the start of text to be copied"
msgstr "Đánh dấu vị trí hiện tại của con trỏ xem lại là điểm bắt đầu của văn bản để chép"

msgid "No start marker set"
msgstr "Không có dấu đánh bắt đầu được thiết lập"

msgid "The start marker must reside within the same object"
msgstr "Dấu đánh bắt đầu phải nằm trong cùng đối tượng"

msgid "Review selection copied to clipboard"
msgstr "Vùng chọn xem lại đã được chép lên bộ nhớ tạm"

msgid "No text to copy"
msgstr "Không có văn bản để chép"

msgid "Retrieves the text from the previously set start marker up to and including the current position of the review cursor and copies it to the clipboard"
msgstr "Lấy văn bản từ đánh dấu bắt đầu đến vị trí hiện tại của con trỏ xem lại và chép lên bộ nhớ tạm"

msgid "Scrolls the braille display back"
msgstr "Cuộn trở lại màn hình chữ nổi"

msgid "Scrolls the braille display forward"
msgstr "Cuộn màn hình chữ nổi về phía trước"

msgid "Routes the cursor to or activates the object under this braille cell"
msgstr "Gửi con trỏ tới hoặc kích hoạt đối tượng dưới ô chữ nổi này"

msgid "Moves the braille display to the previous line"
msgstr "Chuyển màn hình chữ nổi tới dòng trước"

msgid "Moves the braille display to the next line"
msgstr "Chuyển màn hình chữ nổi tới dòng kế"

